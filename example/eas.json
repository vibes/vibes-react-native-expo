{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
       "ANDROID_APP_ID": "3344c960-f53b-43d5-9b3a-2b4498703ef3",
       "IOS_APP_ID": "3344c960-f53b-43d5-9b3a-2b4498703ef3",
       "APP_URL": "https://public-api-uatus0.vibescm.com/mobile_apps",
       "VIBES_APP_ENV": "UAT"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "credentialsSource": "remote"
      }
    },
    "production": {
      "env": {
       "ANDROID_APP_ID": "$ANDROID_APP_ID",
       "IOS_APP_ID": "$IOS_APP_ID",
       "APP_URL": "$APP_URL"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
       "ANDROID_APP_ID": "$ANDROID_APP_ID",
       "IOS_APP_ID": "$IOS_APP_ID",
       "APP_URL": "$APP_URL"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Debug",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {}
  }
} 